<%if logged_in? && current_user.admin == true%>
<h1>These are current users: </h1>
<% @users.each do |user|%>
<div class="user">
  <%=link_to user.name, user_path(user)%>

  <p>
    <%= link_to "Delete", user_path(user), data: {confirm: "Are you sure you want to delete this user?"}, method: :delete%>
    <%=link_to 'Edit', edit_user_path(user)%>
  </p>
  <%end%>
</div>
<% end %>
